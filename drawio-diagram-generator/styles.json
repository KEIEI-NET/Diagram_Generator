  "layoutSettings": {
    "edgeRouting": {
      "strategy": "orthogonal",
      "avoidCrossings": true,
      "minimumSegmentLength": 20,
      "preferredEdgeLength": 80
    },
    "useCase": {
      "actorSpacing": 150,
      "useCaseSpacing": 100,
      "horizontalMargin": 50,
      "verticalMargin": 50
    },
    "class": {
      "classSpacing": 120,
      "horizontalMargin": 50,
      "verticalMargin": 50
    },
    "sequence": {
      "lifelineSpacing": 150,
      "messageSpacing": 50,
      "activationWidth": 10,
      "frameMargin": 10,
      "labelMargin": 10,
      "minElementGap": 20
    },
    "activity": {
      "nodeSpacing": 80,
      "horizontalMargin": 50,
      "verticalMargin": 50
    },
    "er": {
      "entitySpacing": 150,
      "attributeSpacing": 100
    },
    "dataModel": {
      "tableSpacing": 200,
      "columnHeight": 25
    }
  },

  "textRules": {
    "classNames": "preserve",
    "methodNames": "preserve",
    "variableNames": "preserve",
    "labels": "japanese",
    "descriptions": "japanese",
    "comments": "japanese"
  },

  "connectionPointRules": {
    "description": "全ての図形からの矢印は明示的な接続点を持つ（中心接続禁止）",
    "shapes": {
      "rectangle": {
        "top": "exitX=0.5;exitY=0",
        "bottom": "exitX=0.5;exitY=1",
        "left": "exitX=0;exitY=0.5",
        "right": "exitX=1;exitY=0.5"
      },
      "rhombus": {
        "top": "exitX=0.5;exitY=0",
        "bottom": "exitX=0.5;exitY=1",
        "left": "exitX=0;exitY=0.5",
        "right": "exitX=1;exitY=0.5",
        "note": "ひし形は必ず頂点から矢印を出す（中心接続は禁止）"
      },
      "ellipse": {
        "top": "exitX=0.5;exitY=0",
        "bottom": "exitX=0.5;exitY=1",
        "left": "exitX=0;exitY=0.5",
        "right": "exitX=1;exitY=0.5"
      },
      "umlActor": {
        "bottom": "exitX=0.5;exitY=1",
        "right": "exitX=1;exitY=0.3",
        "left": "exitX=0;exitY=0.3",
        "recommended": "bottom"
      },
      "cube": {
        "bottom": "exitX=0.5;exitY=1",
        "right": "exitX=1;exitY=0.5",
        "left": "exitX=0;exitY=0.5"
      }
    },
    "allEdgesMustHave": [
      "exitX",
      "exitY",
      "exitDx",
      "exitDy",
      "entryX",
      "entryY",
      "entryDx",
      "entryDy",
      "labelBackgroundColor",
      "fontColor"
    ]
  },

  "customThemes": {
    "monochrome": {
      "primary": "#E0E0E0",
      "secondary": "#BDBDBD",
      "accent": "#9E9E9E",
      "text": "#212121",
      "border": "#757575"
    },
    "highContrast": {
      "primary": "#000000",
      "secondary": "#FFFFFF",
      "accent": "#FF0000",
      "text": "#000000",
      "border": "#000000"
    },
    "darkMode": {
      "background": "#1E1E1E",
      "primary": "#264653",
      "secondary": "#2A9D8F",
      "accent": "#E76F51",
      "text": "#FFFFFF",
      "border": "#FFFFFF"
    }
  }
